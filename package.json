{
    "name": "@yookue/ts-lang-utils",
    "version": "0.1.3",
    "title": "TsLangUtils",
    "description": "Common Lang Utilities for TypeScript",
    "homepage": "https://yookue.github.io/ts-lang-utils",
    "organization": "Yookue Ltd",
    "private": false,
    "license": "MIT",
    "keywords": [
        "commons-lang",
        "array-utils",
        "boolean-utils",
        "date-utils",
        "json-utils",
        "object-utils",
        "random-utils",
        "string-utils",
        "thread-utils"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "unpkg": "dist/umd/ts-lang-utils.min.js",
    "types": "dist/esm/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "del-cli ./dist && father build",
        "build:watch": "father dev",
        "docs:build": "del-cli ./docs-dist && tsc --outDir ./docs-temp && jsdoc -c jsdoc.config.json && del-cli ./docs-temp",
        "docs:deploy": "gh-pages -d ./docs-dist",
        "doctor": "father doctor",
        "lint": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
        "test": "jest",
        "test:coverage": "jest --coverage"
    },
    "dependencies": {
        "@babel/runtime": "^7.22.15"
    },
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "babel-plugin-comments": "^1.0.4",
        "del-cli": "^5.1.0",
        "docdash": "^2.0.2",
        "eslint": "^7.32.0",
        "father": "^4.3.5",
        "gh-pages": "^6.0.0",
        "jsdoc": "^4.0.2",
        "prettier": "^2.8.8",
        "ts-jest": "^29.1.1",
        "typescript": "^4.9.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yookue/ts-lang-utils.git"
    },
    "bugs": {
        "url": "https://github.com/yookue/ts-lang-utils/issues"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "author": {
        "name": "Yookue Ltd",
        "email": "yookue@outlook.com",
        "url": "https://yookue.com"
    },
    "contributors": [
        {
            "name": "Davis Hsing",
            "email": "davidhsing@163.com",
            "url": "https://github.com/davidhsing"
        }
    ]
}
